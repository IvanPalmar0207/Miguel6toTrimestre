<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".eliminarUsuarios"
    tools:layout_editor_absoluteX="54dp"
    tools:layout_editor_absoluteY="132dp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/roboto_black"
        android:foregroundTint="@color/green"
        android:text="Eliminar Usuario"
        android:textColor="@color/green"
        android:textFontWeight="@integer/material_motion_duration_long_1"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="275dp"
        android:layout_height="26dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="70dp"
        android:fontFamily="@font/roboto_medium"
        android:text="Deseas Eliminar al usuario?"
        android:textAlignment="center"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="@color/green"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.312"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cajaContrasenaEliminarUSU" />

    <Button
        android:id="@+id/btnEliminarUSU"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="72dp"
        android:backgroundTint="#FF9878"
        android:fontFamily="@font/roboto_black"
        android:text="Eliminar"
        android:textColor="@color/black"
        android:textColorHighlight="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnVolverEU"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginTop="56dp"
        android:backgroundTint="#CDDC39"
        android:fontFamily="@font/roboto_black"
        android:text="Volver"
        android:textColor="@color/black"
        android:textColorHighlight="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnVolverCasaEU"
        android:layout_width="60dp"
        android:layout_height="67dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="@color/white"
        app:icon="@drawable/baseline_house_24"
        app:iconGravity="top"
        app:iconSize="55dp"
        app:iconTint="@color/green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="220dp"
        android:layout_height="29dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/roboto_medium"
        android:text="Numero de Documento:"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="@color/green"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/roboto_medium"
        android:text="Correo Electronico:"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="@color/green"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cajaNumeroDocumentoEliminarUSU" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="202dp"
        android:layout_height="27dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/roboto_medium"
        android:text="Ingresa tu ContraseÃ±a"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="@color/green"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cajaCorreoElectronicoEliminarUSU" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="46dp"
        android:layout_height="39dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="14dp"
        app:layout_constraintEnd_toStartOf="@+id/cajaCorreoElectronicoEliminarUSU"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/contrasena" />

    <EditText
        android:id="@+id/cajaContrasenaEliminarUSU"
        android:layout_width="260dp"
        android:layout_height="47dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:fontFamily="@font/roboto_medium"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHighlight="@color/green"
        android:textColorHint="@color/green"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="39dp"
        android:layout_height="41dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toStartOf="@+id/cajaNumeroDocumentoEliminarUSU"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/documento" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="46dp"
        android:layout_height="39dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="14dp"
        app:layout_constraintEnd_toStartOf="@+id/cajaCorreoElectronicoEliminarUSU"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/correo" />

    <EditText
        android:id="@+id/cajaCorreoElectronicoEliminarUSU"
        android:layout_width="260dp"
        android:layout_height="47dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:fontFamily="@font/roboto_medium"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHighlight="@color/green"
        android:textColorHint="@color/green"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/cajaNumeroDocumentoEliminarUSU"
        android:layout_width="263dp"
        android:layout_height="49dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        android:drawablePadding="15dp"
        android:drawableTint="@color/green"
        android:drawingCacheQuality="high"
        android:ems="10"
        android:fontFamily="@font/roboto_medium"
        android:inputType="text"
        android:padding="15dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHighlight="@color/green"
        android:textColorHint="@color/green"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

</androidx.constraintlayout.widget.ConstraintLayout>